    MaNV INT PRIMARY KEY,
    HoTen VARCHAR(50),
    Tuoi INT,
    PhongBan VARCHAR(50)
);
INSERT INTO NhanVien (MaNV, HoTen, Tuoi, PhongBan) 
VALUES 
    (1, 'Nguyễn Văn A', 30, 'Kế toán'),
    (2, 'Trần Thị B', 28, 'Nhân sự'),
    (3, 'Lê Văn C', 35, 'IT'),
    (4, 'Phạm Thị D', 26, 'Marketing'),
    (5, 'Hoàng Văn E', 40, 'Kinh doanh');
SELECT * FROM NhanVien;
SELECT *  
FROM NhanVien  
WHERE Tuoi = (  
    SELECT MAX(Tuoi)  
    FROM NhanVien AS NV  
    WHERE NV.PhongBan = NhanVien.PhongBan  
);
SELECT *  
FROM NhanVien  
WHERE Tuoi &gt; 25;
SELECT HoTen, Tuoi  
FROM NhanVien  
WHERE PhongBan = 'IT';




